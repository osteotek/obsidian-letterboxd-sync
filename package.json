{
	"name": "obsidian-letterboxd-sync",
	"version": "1.0.0",
	"description": "Import movie data from Letterboxd CSV exports and create markdown notes with posters.",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"test": "vitest run",
		"version": "node version-bump.mjs && git add manifest.json versions.json",
		"release": "scripts/prepare-release.sh"
	},
	"keywords": [
		"obsidian",
		"letterboxd",
		"movies",
		"import"
	],
	"author": "osteotek",
	"license": "MIT",
	"devDependencies": {
		"@types/node": "^20.14.11",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"obsidian": "latest",
		"tslib": "2.4.0",
		"typescript": "4.7.4",
		"vitest": "^1.6.0"
	}
}
